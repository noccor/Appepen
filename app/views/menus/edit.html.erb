<div class="panel panel-default">
  <div class="panel-heading">
    Edit
  </div>
<div class="panel-body">
  <%= form_for @menu do |f| %>
    <%= f.label :name %>:
    <%= f.text_field :name, class: "form-control" %><br />

    <%= f.label :description %>:
    <%= f.text_field :description, class: "form-control" %><br />

    <%= f.label :cat %>:
    <%= f.text_field :cat, class: "form-control" %><br />

    <%= f.submit %>
  <% end %>
</div>
